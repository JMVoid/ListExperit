<UserControl x:Class="ListExperit.View.StudnentPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:ListExperit.View"
             mc:Ignorable="d" 
             Background="White"
             DataContext="{Binding Source={StaticResource Locator}, Path=Student}"
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Label Content="The Starting"></Label>
        <Label Content="{Binding StudentModel.Id}" HorizontalAlignment="Left" Margin="266,134,0,0" VerticalAlignment="Top"/>
        <Label Content="{Binding StudentModel.Name}" HorizontalAlignment="Left" Margin="274,191,0,0" VerticalAlignment="Top"/>


    </Grid>
</UserControl>
